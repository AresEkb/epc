<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated epc -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="epc.presentation.EpcModelWizardID"
            name="%_UI_EpcModelWizard_label"
            class="epc.presentation.EpcModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/EpcModelFile.gif">
         <description>%_UI_EpcModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated epc -->
      <editor
            id="epc.presentation.EpcEditorID"
            name="%_UI_EpcEditor_label"
            icon="icons/full/obj16/EpcModelFile.gif"
            extensions="epc"
            class="epc.presentation.EpcEditor"
            contributorClass="epc.presentation.EpcActionBarContributor">
      </editor>
   </extension>
</plugin>
